{
  "name": "node-red-contrib-bit",
  "version": "0.0.3",
  "description": "Bit helper contribution package for Node-RED.",
  "dependencies": {
    "debug": "3.1.0"
  },
  "keywords": [
    "node-red",
    "Bit",
    "bit",
    "converter",
    "split"
  ],
  "engines": {
    "node": ">=4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biancode/node-red-contrib-bit.git"
  },
  "node-red": {
    "nodes": {
      "Bit-Simple-Sixteen": "bit/bit-simple-sixteen.js",
      "Bit-Sixteen": "bit/bit-sixteen.js",
      "Bit-Simple-Thirtytwo": "bit/bit-simple-thirtytwo.js",
      "Bit-Thirtytwo": "bit/bit-thirtytwo.js"
    }
  },
  "author": "Klaus Landsdorf",
  "license": "BSD-3-Clause",
  "main": "node",
  "files": [
    "bit"
  ],
  "bugs": {
    "url": "https://github.com/biancode/node-red-contrib-bit/issues/"
  },
  "homepage": "https://biancode.github.io/node-red-contrib-bit/",
  "scripts": {
    "test": "standard && mocha test/*",
    "build": "gulp publish",
    "prepublish": "npm run build && npm test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-uglify": "^1.5.3",
    "jasmine-node": "^1.14.5",
    "js-beautify": "^1.6.4",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "node-red": "^0.14.6",
    "pump": "^1.0.2",
    "should": "^8.3.2",
    "sinon": "^1.17.4",
    "standard": "^8.6.0",
    "supertest": "^1.2.0",
    "when": "^3.7.7"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "examples/",
      "bit/",
      "docs",
      "test",
      "extras"
    ]
